<div class="panel panel-primary">
  <div class="panel-heading">Usuarios</div>
<table class="table">
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Username</th>
      <th>Contrase√±a</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Apellido</th>
      <th>Rut</th>
      <th>email</th>
      <th>VIP</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.usuario %></td>
        <td><%= usuario.usuario_nombre_usuario %></td>
        <td><%= usuario.usuario_contrasena %></td>
        <td><%= usuario.usuario_nombre %></td>
        <td><%= usuario.usuario_apellidopat %></td>
        <td><%= usuario.usuario_apellidomat %></td>
        <td><%= usuario.usuario_rut %></td>
        <td><%= usuario.usuario_e_mail %></td>
        <td><%= usuario.usuario_vip %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

